S_0x56560507f3e0 .scope module, "pwm_tb" "pwm_tb" 2 3;
 .timescale -9 -12;
v0x565605094c20_0 .var "clk", 0 0;
v0x565605094ce0_0 .net "pwm_out", 0 0, v0x565605067ad0_0;  1 drivers
v0x565605094d80_0 .var "rst", 0 0;
v0x565605094e20_0 .var "sw", 2 0;
S_0x56560507f570 .scope module, "uut" "pwm" 2 11, 3 22 0, S_0x56560507f3e0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 3 "sw";
    .port_info 3 /OUTPUT 1 "pwm_out";
v0x565605066f30_0 .net "clk", 0 0, v0x565605094c20_0;  1 drivers
v0x565605067030_0 .var "counter", 3 0;
v0x565605067a00_0 .var "duty_cycle", 3 0;
v0x565605067ad0_0 .var "pwm_out", 0 0;
v0x5656050949b0_0 .net "rst", 0 0, v0x565605094d80_0;  1 drivers
v0x565605094ac0_0 .net "sw", 2 0, v0x565605094e20_0;  1 drivers
E_0x5656050793c0 .event posedge, v0x5656050949b0_0, v0x565605066f30_0;
    .scope S_0x56560507f570;
T_0 ;
    %wait E_0x5656050793c0;
    %load/vec4 v0x5656050949b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x565605094ac0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
