    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 3;
    %cmp/u;
    %jmp/1 T_0.8, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 3;
    %cmp/u;
    %jmp/1 T_0.9, 6;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.2 ;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.3 ;
    %pushi/vec4 2, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.4 ;
    %pushi/vec4 3, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.5 ;
    %pushi/vec4 4, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.6 ;
    %pushi/vec4 5, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.7 ;
    %pushi/vec4 6, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.8 ;
    %pushi/vec4 7, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.9 ;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x565605067a00_0, 0;
    %jmp T_0.11;
T_0.11 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x56560507f570;
T_1 ;
    %wait E_0x5656050793c0;
    %load/vec4 v0x5656050949b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x565605067030_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x565605067030_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x565605067030_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x565605067030_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x565605067030_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x56560507f570;
T_2 ;
    %wait E_0x5656050793c0;
    %load/vec4 v0x5656050949b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x565605067ad0_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x565605067030_0;
    %load/vec4 v0x565605067a00_0;
