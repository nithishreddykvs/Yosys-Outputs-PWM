    .scope S_0x56560507f3e0;
T_4 ;
    %vpi_call 2 23 "$dumpfile", "pwm.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x56560507f3e0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x565605094c20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x565605094d80_0, 0, 1;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x565605094e20_0, 0, 3;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x565605094d80_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x565605094e20_0, 0, 3;
    %delay 200000, 0;
